{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/messages",
      "dest": "./index.js"
    },
    {
      "src": "/api/delete/messages",
      "dest": "./index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
